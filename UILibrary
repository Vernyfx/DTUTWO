				title_noticenter.Active = true
				title_noticenter.AnchorPoint = Vector2.new(0.5, 0.5)
				title_noticenter.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				title_noticenter.BackgroundTransparency = 1.000
				title_noticenter.Position = UDim2.new(0.495000005, 0, 0.105999999, 0)
				title_noticenter.Size = UDim2.new(0, 265, 0, 28)
				title_noticenter.FontFace = Font.fromId(12187365977,Enum.FontWeight.Bold)
				title_noticenter.Text = tostring(Title)
				title_noticenter.TextColor3 = Color3.fromRGB(255, 255, 255)
				title_noticenter.TextSize = 14.000
				title_noticenter.TextWrapped = true
				title_noticenter.TextTransparency = 1

				desc_noticenter.Name = "desc_noticenter"
				desc_noticenter.Parent = notificationcenter_main
				desc_noticenter.Active = true
				desc_noticenter.AnchorPoint = Vector2.new(0.5, 0.5)
				desc_noticenter.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				desc_noticenter.BackgroundTransparency = 1.000
				desc_noticenter.Position = UDim2.new(0.495000005, 0, 0.495999992, 0)
				desc_noticenter.Size = UDim2.new(0, 265, 0, 151)
				desc_noticenter.FontFace = Font.fromId(12187365977)
				desc_noticenter.Text = tostring(Desc)
				desc_noticenter.TextColor3 = Color3.fromRGB(255, 255, 255)
				desc_noticenter.TextSize = 14.000
				desc_noticenter.TextTransparency = 1
				desc_noticenter.TextWrapped = true
				desc_noticenter.TextYAlignment = Enum.TextYAlignment.Top

				closebutton_centernoti.Name = "closebutton_centernoti"
				closebutton_centernoti.Parent = notificationcenter_main
				closebutton_centernoti.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
				closebutton_centernoti.BorderSizePixel = 0
				closebutton_centernoti.Position = UDim2.new(0.0566666685, 0, 0.860000014, 0)
				closebutton_centernoti.Size = UDim2.new(0, 266, 0, 23)
				closebutton_centernoti.FontFace = Font.fromId(12187365977,Enum.FontWeight.Bold)
				closebutton_centernoti.Text = "Submit"
				closebutton_centernoti.TextColor3 = Color3.fromRGB(255, 255, 255)
				closebutton_centernoti.TextSize = 12.000
				closebutton_centernoti.TextTransparency = 1
				closebutton_centernoti.TextWrapped = true
				closebutton_centernoti.BackgroundTransparency = 1
				closebutton_centernoti.AutoButtonColor = false
				closebutton_centernoti.ClipsDescendants = true

				stroke(closebutton_centernoti,0,1,Color3.fromRGB(50,50,50))

				UICorner.CornerRadius = UDim.new(0, 4)
				UICorner.Parent = closebutton_centernoti

				line_fill_noti.Name = "line_fill_noti"
				line_fill_noti.Parent = notificationcenter_main
				line_fill_noti.Active = true
				line_fill_noti.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
				line_fill_noti.BorderSizePixel = 0
				line_fill_noti.Position = UDim2.new(0.0599999987, 0, 0.808000028, 0)
				line_fill_noti.Size = UDim2.new(0, 263, 0, 6)
				line_fill_noti.BackgroundTransparency = 1

				line_fill_noti_uicorner.Name = "line_fill_noti_uicorner"
				line_fill_noti_uicorner.Parent = line_fill_noti

				line_color.Name = "line_color"
				line_color.Parent = notificationcenter_main
				line_color.Active = true
				line_color.BackgroundColor3 = Color3.fromRGB(100, 200, 239)
				line_color.BorderSizePixel = 0
				line_color.Position = UDim2.new(0.0599999987, 0, 0.808000028, 0)
				line_color.Size = UDim2.new(0, 263, 0, 6)
				line_color.BackgroundTransparency = 1

				line_color_uicorner.Name = "line_color_uicorner"
				line_color_uicorner.Parent = line_color

				dropshadow.Name = "dropshadow"
				dropshadow.Parent = notificationcenter
				dropshadow.Active = true
				dropshadow.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				dropshadow.BackgroundTransparency = 1.000
				dropshadow.Position = UDim2.new(0.228373706, 0, 0.239999995, 0)
				dropshadow.Size = UDim2.new(0, 315, 0, 263)
				dropshadow.ZIndex = 0
				dropshadow.Image = "8068653048"
				dropshadow.ImageColor3 = Color3.fromRGB(0, 0, 0)
				dropshadow.ImageTransparency = 1

				closebutton_centernoti.MouseButton1Down:Connect(function()
					CircleAnim(closebutton_centernoti,Color3.fromRGB(255,255,255),Color3.fromRGB(255,255,255))
					tween(dropshadow,0.5,Enum.EasingStyle.Quart,{ImageTransparency = 1})
					tween(desc_noticenter,0.5,Enum.EasingStyle.Quart,{TextTransparency = 1})
					tween(title_noticenter,0.5,Enum.EasingStyle.Quart,{TextTransparency = 1})
					tween(line_fill_noti,0.5,Enum.EasingStyle.Quart,{BackgroundTransparency = 1})
					tween(line_color,0.5,Enum.EasingStyle.Quart,{BackgroundTransparency = 1})
					tween(closebutton_centernoti,0.5,Enum.EasingStyle.Quart,{TextTransparency = 1})
					tween(closebutton_centernoti,0.5,Enum.EasingStyle.Quart,{BackgroundTransparency = 1})
					repeat wait() until closebutton_centernoti.BackgroundTransparency == 1
					tween(notificationcenter,0.5,Enum.EasingStyle.Quart,{Size = UDim2.new(0, 0, 0, 0)})
					wait(.2)
					notificationcenter:Destroy()
				end)

				repeat wait() until notificationcenter.Size == UDim2.new(0, 578, 0, 500)
				tween(notificationcenter_main,0.5,Enum.EasingStyle.Quart,{Size = UDim2.new(0, 300, 0, 250)})
				repeat wait() until notificationcenter_main.Size == UDim2.new(0, 300, 0, 250)
				tween(dropshadow,0.5,Enum.EasingStyle.Quart,{ImageTransparency = 0})
				tween(desc_noticenter,0.5,Enum.EasingStyle.Quart,{TextTransparency = 0.450})
				tween(title_noticenter,0.5,Enum.EasingStyle.Quart,{TextTransparency = 0})
				tween(line_fill_noti,0.5,Enum.EasingStyle.Quart,{BackgroundTransparency = 0})
				tween(line_color,0.5,Enum.EasingStyle.Quart,{BackgroundTransparency = 0})
				tween(closebutton_centernoti,0.5,Enum.EasingStyle.Quart,{TextTransparency = 0.45})
				tween(closebutton_centernoti,0.5,Enum.EasingStyle.Quart,{BackgroundTransparency = 0})
				repeat wait() until closebutton_centernoti.BackgroundTransparency == 0
				tween(line_color,tonumber(Cooldown),Enum.EasingStyle.Quart,{Size = UDim2.new(0, 0, 0, 6)})
				delay(tonumber(Cooldown),function()
					tween(dropshadow,0.5,Enum.EasingStyle.Quart,{ImageTransparency = 1})
					tween(desc_noticenter,0.5,Enum.EasingStyle.Quart,{TextTransparency = 1})
					tween(title_noticenter,0.5,Enum.EasingStyle.Quart,{TextTransparency = 1})
					tween(line_fill_noti,0.5,Enum.EasingStyle.Quart,{BackgroundTransparency = 1})
					tween(line_color,0.5,Enum.EasingStyle.Quart,{BackgroundTransparency = 1})
					tween(closebutton_centernoti,0.5,Enum.EasingStyle.Quart,{TextTransparency = 1})
					tween(closebutton_centernoti,0.5,Enum.EasingStyle.Quart,{BackgroundTransparency = 1})
					repeat wait() until closebutton_centernoti.BackgroundTransparency == 1
					tween(notificationcenter,0.5,Enum.EasingStyle.Quart,{Size = UDim2.new(0, 0, 0, 0)})
					wait(.2)
					notificationcenter:Destroy()
				end)
			end
		end

